{
 "extends": "rallycoding",
  "plugins": [
    "react"
  ],
 "env": {
   "browser": true,
   "node": true,
   "mocha": true,
  "jest": true
  },
  "rules": {
    "arrow-body-style": 0,
    "array-callback-return": 0,
    "import/no-named-as-default": 0,
    "max-len": 0,
    "import/no-duplicates": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0
  }
}
